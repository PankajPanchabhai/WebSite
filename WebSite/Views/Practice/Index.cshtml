
@{
    ViewBag.Title = "Practice";
}
<html>

<head>
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Content/Pages/Practice.js"></script>
</head>



<body>
    <h3>Student Details</h3>
    <div class="container">

        <div class="row">

            <div class="col col-md-4">

                <input type="hidden" id="hdId" />

                <label>Name</label>
                <input type="text" id="txtname" placeholder="Enter Name here" class="form-control" />

                <br /><br />


                <label>Contact</label>
                <input type="text" id="txtPhone" placeholder="Enter contact Number " class="form-control" />
                <br /><br />


                <label>Photo</label>

                <input type="file" id="Photo" class="form-control" />

            </div>
        </div>
    </div>
    <br /><br />
    <input type="submit" class="btn btn-primary" onclick="save()" />


    <br />
    <br />
    <br />
    <div>
        <table id="tblPractice" class="table table-striped">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Phone</th>
                    <th>Photo</th>
                    <th></th>
                    <th></th>
                    <th></th>
                </tr>
            </thead>


            <tbody></tbody>
        </table>
    </div>


</body>
</html>